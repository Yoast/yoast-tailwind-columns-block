{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "yoast/columns",
	"title": "Yoast tailwind columns",
	"category": "layout",
	"icon": "columns",
	"textdomain": "yoast-tailwind-columns",
	"example": {},
	"attributes": {
		"columns": {
			"type": "integer"
		},
		"layoutSwitch": {
			"type": "boolean",
			"default": false
		},
		"flexDirection": {
			"type": "string",
			"default": "row"
		},
		"breakpointDirection": {
			"type": "string",
			"default": "col"
		},
		"flexWrap": {
			"type": "boolean",
			"default": true
		}
	},
	"editorScript": "file:./build/index.js"
}
